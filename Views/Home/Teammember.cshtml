@{
    ViewData["Title"] = "Teammember";
}
@model List<Teammember>

<section id="doctors" class="doctors section-bg">
    <div class="container aos-init aos-animate" data-aos="fade-up">

        <div class="section-title">
            <h2>Takım üyeleri</h2>
          
        </div>
     
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-3 col-md-6 d-flex align-items-stretch" style="width:100%">

                    <div class="member aos-init aos-animate" data-aos="fade-up" data-aos-delay="200">
                        <div class="member-img">
                             
                           
                            <img src="~/img/news/@item.Image" height="640" width="640" class="img-fluid" >
                            <div class="social">
                                <a href="@item.twitter"><i class="icofont-twitter"></i></a>
                                <a href="@item.Facebook"><i class="icofont-facebook"></i></a>
                                <a href="@item.instagram"><i class="icofont-instagram"></i></a>
                                <a href="@item.whatsapp"><i class="fab fa-whatsapp"></i></a>
                            </div>
                        </div>
                        <div class="member-info">

                            <h4>@item.Name</h4>
                            <span>@item.Jop</span>


                        </div>
                    </div>
                </div>
            }


        </div>
    </div>

   

    
</section>


